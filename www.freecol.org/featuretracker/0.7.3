<script type="text/javascript">
    /*global Handlebars*/
    $(function($) {
        var source = $('#jobwidget-template').html();
        var template = Handlebars.compile(source);

        Handlebars.registerHelper('stripe', function(array, display_count, random, fn) {
            var buffer = "";
            var max_iterations = Math.min(array.length,display_count);
            for (var i = 0, len = array.length; i < max_iterations; i++) {
                var item = null;
                if (random === true) {
                    var ri = Math.floor((Math.random()*array.length)+1);
                    item = array[ri];
                } else {
                    item = array[i];
                }
                if (item) {
                    item.css_class = (i % 2 === 0 ? 'even' : 'odd');
                }
                buffer += fn.fn(item);
            }
            return buffer;
        });

        function load_jobBoard() {
            $.ajax({
                url:'/jobs/json',
                success: function(data) {
                    $('#job-listing').html(template(data));
                    var provider_name = data.provider_name, attrib_id;
                    switch (provider_name) {
                        case 'itjb':
                            var itjb_link = 'http://www.' + data.itjb_links["NO"] + '?' + data.itjb_tracker.logo;
                            $('#itjb_at a').prop('href', itjb_link);
                            attrib_id = '#itjb_at';
                            $('#job-listing li a').each(function (index, element) {
                                var href = $(element).prop('href');
                                href += '&' + data.itjb_tracker.link;
                                $(element).prop('href', href);
                            });$('#itjb-logo-container').html('<img src="http://a.fsdn.com/con/img/jobs/ITJB_no.jpg" class="provider-logo" alt="ITJB job search" />');break;
                        case 'indeed':
                            attrib_id = '#indeed_at';
                        break;
                        default:
                            attrib_id = '#dice_at';
                        break;
                    }
                    $(attrib_id).show().siblings().hide();
                },
                dataType: 'json'
            });
        }
        load_jobBoard();
    });
</script>

